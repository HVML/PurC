#!/usr/bin/purc

# RESULT: [0L,1L,2L,3L,4L,6L,7L,8L,9L]

<!DOCTYPE hvml>
<hvml target="void">

    <body>
        <init as result with [] />

        <iterate on 0L onlyif $L.lt($0<, 10L) with (_ipt[0] + 1L) nosetotail >
            <test with $L.eq($?, 5)>
                <back to="1"/>
            </test>
            <update on $result to 'append' with $? />
        </iterate>

        <exit with $result />
    </body>
</hvml>

