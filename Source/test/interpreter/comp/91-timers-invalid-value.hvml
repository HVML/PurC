#!/usr/bin/purc

# RESULT: 'Ok'

<!DOCTYPE hvml>
<hvml target="html" lang="en">
<body>
    <inherit on $RUNNER.enablelog('info') />

    <init as badTimers>
        [
            { 'interval' : null,  },
            { "id" : "clock",  },
            { "id" : "clock", "interval": null },
            { "id" : "clock", "active": 55 },
            { "id" : 9, "active": true },
            { "id" : "9 9", "active": false },
        ]
    </init>

    <iterate on $badTimers>
        <update on $TIMERS to unite with [$?] >
            <catch for `ANY`>
                <test with $L.strne('case', $?.name, 'InvalidValue') >
                    <exit with 'Failed' />
                </test>
            </catch>
        </update>
    </iterate>

    <exit with 'Ok' />
</body>
</hvml>

