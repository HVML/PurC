#!/usr/bin/purc

# RESULT: [0L, 0L, 0L, 0L, 0L]

<!DOCTYPE hvml>
<hvml target="void">

    <init as set uniquely>
        [ 1, 2, 3, 4, 5, 6, 7 ]
    </init>

    <observe on $set for 'change:deflated'>
        <exit with $? />
    </observe>

    <iterate on 0 onlyif $L.lt($0<, 5) with (_ipt[0] + 1) nosetotail >
      <update on $set at 0 to remove />
    </iterate>
</hvml>
