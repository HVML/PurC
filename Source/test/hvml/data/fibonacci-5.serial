PCHVML_TOKEN_DOCTYPE|<!DOCTYPE hvml SYSTEM "v: MATH">
PCHVML_TOKEN_VCM_TREE|"
"
PCHVML_TOKEN_START_TAG|<hvml target="html" lang="en">
PCHVML_TOKEN_VCM_TREE|"
    "
PCHVML_TOKEN_START_TAG|<head>
PCHVML_TOKEN_VCM_TREE|"
        "
PCHVML_TOKEN_START_TAG|<title>
PCHVML_TOKEN_VCM_TREE|"Fibonacci Numbers"
PCHVML_TOKEN_END_TAG|</title>
PCHVML_TOKEN_VCM_TREE|"
    "
PCHVML_TOKEN_END_TAG|</head>
PCHVML_TOKEN_VCM_TREE|"

    "
PCHVML_TOKEN_START_TAG|<body>
PCHVML_TOKEN_VCM_TREE|"
        "
PCHVML_TOKEN_START_TAG|<header>
PCHVML_TOKEN_VCM_TREE|"
            "
PCHVML_TOKEN_START_TAG|<h1>
PCHVML_TOKEN_VCM_TREE|"Fibonacci Numbers less than 2000"
PCHVML_TOKEN_END_TAG|</h1>
PCHVML_TOKEN_VCM_TREE|"
            "
PCHVML_TOKEN_START_TAG|<p hvml:raw>
PCHVML_TOKEN_VCM_TREE|"Using local variables and while and with attributes"
PCHVML_TOKEN_END_TAG|</p>
PCHVML_TOKEN_VCM_TREE|"
        "
PCHVML_TOKEN_END_TAG|</header>
PCHVML_TOKEN_VCM_TREE|"

        "
PCHVML_TOKEN_START_TAG|<init as="count" with=2 temporarily/>
PCHVML_TOKEN_VCM_TREE|"

        "
PCHVML_TOKEN_START_TAG|<section>
PCHVML_TOKEN_VCM_TREE|"
            "
PCHVML_TOKEN_START_TAG|<dl>
PCHVML_TOKEN_VCM_TREE|"
                "
PCHVML_TOKEN_START_TAG|<init as="last_one" with=0 temporarily/>
PCHVML_TOKEN_VCM_TREE|"
                "
PCHVML_TOKEN_START_TAG|<init as="last_two" with=1 temporarily/>
PCHVML_TOKEN_VCM_TREE|"

                "
PCHVML_TOKEN_START_TAG|<iterate on=1 onlyif=callGetter(getElement(getVariable("L"), "lt"), getVariable("0<"), 2000) with=callGetter(getElement(getVariable("MATH"), "add"), getVariable("0<"), getElement(getVariable("!"), "last_one"))>
PCHVML_TOKEN_VCM_TREE|"
                    "
PCHVML_TOKEN_START_TAG|<init at="last_one" with=getElement(getVariable("1!"), "last_two") temporarily/>
PCHVML_TOKEN_VCM_TREE|"
                    "
PCHVML_TOKEN_START_TAG|<init at="last_two" with=getVariable("?") temporarily/>
PCHVML_TOKEN_VCM_TREE|"

                    "
PCHVML_TOKEN_START_TAG|<update on=getVariable("3!") at=".count" to="displace" with+=1/>
PCHVML_TOKEN_VCM_TREE|"
                    "
PCHVML_TOKEN_START_TAG|<dt>
PCHVML_TOKEN_VCM_TREE|getVariable("%")
PCHVML_TOKEN_END_TAG|</dt>
PCHVML_TOKEN_VCM_TREE|"
                    "
PCHVML_TOKEN_START_TAG|<dd>
PCHVML_TOKEN_VCM_TREE|getVariable("?")
PCHVML_TOKEN_END_TAG|</dd>
PCHVML_TOKEN_VCM_TREE|"
                "
PCHVML_TOKEN_END_TAG|</iterate>
PCHVML_TOKEN_VCM_TREE|"
            "
PCHVML_TOKEN_END_TAG|</dl>
PCHVML_TOKEN_VCM_TREE|"
        "
PCHVML_TOKEN_END_TAG|</section>
PCHVML_TOKEN_VCM_TREE|"

        "
PCHVML_TOKEN_START_TAG|<footer>
PCHVML_TOKEN_VCM_TREE|"
            "
PCHVML_TOKEN_START_TAG|<p>
PCHVML_TOKEN_VCM_TREE|concatString("Totally ", getElement(getVariable("1!"), "count"), " numbers.")
PCHVML_TOKEN_END_TAG|</p>
PCHVML_TOKEN_VCM_TREE|"
        "
PCHVML_TOKEN_END_TAG|</footer>
PCHVML_TOKEN_VCM_TREE|"
    "
PCHVML_TOKEN_END_TAG|</body>
PCHVML_TOKEN_VCM_TREE|"

"
PCHVML_TOKEN_END_TAG|</hvml>
